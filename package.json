{
  "name": "service-cron",
  "version": "0.0.0",
  "description": "Provides cronjobs through a web worker",
  "private": false,
  "main": "./lib/main.js",
  "typings": "./types",
  "scripts": {
    "build": "node ./tools/build.mjs",
    "clean": "rimraf ./lib",
    "dev": "npm run start",
    "lint:ts": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all lint:*",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "cross-env ESBUILD_WATCH=true node ./tools/build.mjs",
    "test": "npm run lint"
  },
  "keywords": [
    "cron",
    "cronjob",
    "scheduler",
    "scheduled task",
    "scheduled tasks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-service-cron"
  },
  "homepage": "https://atom.io/packages/service-cron",
  "bugs": {
    "url": "https://github.com/idleberg/atom-service-cron/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "service-cron": {
      "description": "Provides a cron service for Atom packages",
      "versions": {
        "0.1.0": "provideCron"
      }
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@atxm/developer-console": "^0.5.0",
    "@types/atom": "^1.40.5",
    "@types/node": "^14.14.43",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.12.9",
    "eslint": "^7.25.0",
    "eslint-plugin-json": "^2.1.2",
    "husky": ">=4 <5",
    "jsonlint": "^1.6.3",
    "lint-staged": "^10.5.3",
    "log-symbols": "^5.0.0",
    "node-cron": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "source-map-explorer": "^2.5.1",
    "stylelint": "^13.13.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "volta": {
    "node": "14.17.1",
    "yarn": "1.22.10"
  }
}
